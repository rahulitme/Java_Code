class Main {
    public static void main(String[] args) {
         int k = 2;
        int arr[] = {1,2,3,4,56};
        int n = arr.length;
          k = k % n;
        for(int i = n-k;i<n;i++)
        {
             System.out.println(arr[i]);
        }
            for(int j =0;j<= n-k;j++)
            {
                System.out.println(arr[j]);
            }
                
        }
    } 4,56,1,2,3

import java.util.Scanner;

public class Main {
    public static boolean isPowerOfTwo(int n) {
        return n > 0 && (n & (n - 1)) == 0;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number: ");
        int num = sc.nextInt();

        if (isPowerOfTwo(num))
            System.out.println(num + " is a power of 2.");
        else
            System.out.println(num + " is NOT a power of 2.");
    }
}
 power of two or not

class Main {
    public static void main(String[] args) {
        int arr[] = {1, 2, 3, 2, 32};
        int temp;

        // Bubble Sort logic
        for (int i = 0; i < arr.length - 1; i++) {
            for (int j = 0; j < arr.length - i - 1; j++) {
                if (arr[j] > arr[j + 1]) {
                    // swap arr[j] and arr[j + 1]
                    temp = arr[j];
                    arr[j] = arr[j + 1];
                    arr[j + 1] = temp;
                }
            }
        }

        // Print sorted array
        System.out.println("Sorted array:");
        for (int i = 0; i < arr.length; i++) {
            System.out.println(arr[i]);
        }
    }
}

import java.util.Arrays;

class Main {
    public static void main(String[] args) {
        String s1 = "aaman";
        
        // Convert string to lowercase if you want case-insensitive uniqueness
        char[] ch = s1.toCharArray();
        Arrays.sort(ch); // Sort the array once

        StringBuilder result = new StringBuilder();
        result.append(ch[0]); // Add first character

        for (int i = 1; i < ch.length; i++) {
            if (ch[i] != ch[i - 1]) {
                result.append(ch[i]); // Append only if it's not a duplicate
            }
        }

        System.out.println(result.toString());
    }
}
public class Main {
    public static void main(String[] args) {
        int[] arr = {2, 3, 2, 4, 32, 4};

        for (int i = 0; i < arr.length; i++) {
            for (int j = i + 1; j < arr.length; j++) {
                for (int k = j + 1; k < arr.length; k++) {
                    System.out.println("Triplet: " + arr[i] + ", " + arr[j] + ", " + arr[k]);
                }
            }
        }

        System.out.println("Try programiz.pro");
    }
}


