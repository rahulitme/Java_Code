        char ch2[] = s2.toCharArray();
        
        Arrays.sort(ch);
        Arrays.sort(ch2);
        if(Arrays.equals(ch,ch2))
        {
            System.out.println("this is anagram");
        }
        else{
            System.out.println("this is  not anagram");
            
        }
    }
}

