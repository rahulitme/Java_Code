class Main {
    public static void main(String[] args) {
        String s = "rahulKuMaR"; // Input string
        int uppercase = 0;       // Counter for uppercase letters
        int lowercase = 0;       // Counter for lowercase letters
        
        // Loop through the string
        for (int i = 0; i < s.length(); i++) {
            char ch = s.charAt(i); // Get the character at the current index
            
            if (Character.isUpperCase(ch)) {   // Check if the character is uppercase
                uppercase++;
            } else if (Character.isLowerCase(ch)) { // Check if the character is lowercase
                lowercase++;
            }
        }
        
        // Print the counts after the loop
        System.out.println("Uppercase Count: " + uppercase);
        System.out.println("Lowercase Count: " + lowercase);
    }
}
//print only the Uppercase character
class Main {
    public static void main(String[] args) {
        String s = "skYBlue";
        String upppercase = "";
        
        for(int i = 0;i<s.length();i++)
        {
            char c = s.charAt(i);
            if(Character.isUpperCase(c))
            {
                upppercase = upppercase+c;
            }
        }
        
        System.out.println(upppercase);
    }
}

class Main {
    public static void main(String[] args) {
        String s = "skYBlue";
        String uppercase = ""; // Fixed spelling of "uppercase"
        
        for (int i = s.length() - 1; i >= 0; i--) { // Corrected "length" to "s.length()"
            char c = s.charAt(i);
            if (Character.isUpperCase(c)) {
                uppercase = uppercase + c; // Appending uppercase letters
            }
        }
        System.out.println(uppercase); // Output the collected uppercase letters
    }
}
