import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        int[] arr = {1, 2, 3, 4, 5};
        int n = 2; // Number of rotations

        // Simple right rotation using your approach
        for (int i = 0; i < n; i++) {
            int first = arr[0]; // Store first element
            
            for (int j = 0; j < arr.length - 1; j++) { // Shift elements left
                arr[j] = arr[j + 1];
            }
            
            arr[arr.length - 1] = first; // Move first element to the end
        }

        // Print the rotated array
        System.out.println("Left Rotated Array: " + Arrays.toString(arr));
    }
}
import java.util.Arrays;

public class Main {
    public static void main(String[] args) {
        int[] arr = {10, 20, 5, 8, 30, 25};

        // Sorting the array
        Arrays.sort(arr);
        
        // Printing the sorted array
        System.out.println("Sorted array: " + Arrays.toString(arr));

        // Printing the second largest element
        System.out.println("Second largest number: " + arr[arr.length - 3]);
    }
}

import java.util.Arrays;

class Main {
    public static void main(String[] args) {
        int arr[] = {1, 2, 0, 3, 4, 5, 0, 0};
        int temp; 
        
        // Iterate and move non-zero numbers to the front
        int j = 0; // Pointer for placing non-zero numbers
        
        for (int i = 0; i < arr.length; i++) {
            if (arr[i] != 0) {
                temp = arr[i];
                arr[i] = arr[j];
                arr[j] = temp;
                j++; // Move the pointer
            }
        }
        
        System.out.println(Arrays.toString(arr));
    }
}
