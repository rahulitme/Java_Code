class Main {
    public static void main(String[] args) {
        String s1 = "rahul";
        String s2 = "kumar";
        char ch[] = s1.toCharArray();
         char ch1[] = s2.toCharArray();
        for(int i =0;i<s1.length()-2;i++)
        {
        System.out.println(ch[i]);
        }
        
       for(int j=s2.length()-3;j>=0;j--)
        {
        System.out.println(ch1[j]);
        }
    }
