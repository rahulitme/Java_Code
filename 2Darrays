public class TwoByTwoMatrix {
    public static void main(String[] args) {
    int matrix[][]=[2][3];

    matrix[0][0]=1;
    matrix[0][1]=1;
     matrix[1][0]=1;
     matrix[1][1]=1;

    printmatrix(matrix)

    }

    static void printmatrix(int matrix[][])
    {
    for(int i=0;i<matrix.length;i++)
    {
    for(int j=0;i<matrix[0].length;i++)
    {
    System.out.println(matrix[i][j]+" ")
    }
    System.out.println()
}
    }
}
