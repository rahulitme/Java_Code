class Main {
    public static void main(String[] args) {
        // Declare and initialize the integer array
        int a[] = {1, 2, 3, 4, 3, 5, 4};

        // Create an Object array to accommodate integers and a String
        Object []p = new Object[a.length+1];
        for(int i =0;i<a.length;i++)
        {
            p[i] = a[i];
        }
        p[a.length] = "solo";
        
        for(int i = 0;i<p.length;i++)
        {
            System.out.println(p[i]);
        }
    }
} //1,2,3,4,3,5,4 solo


